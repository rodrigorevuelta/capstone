{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "namespace":"stockprice",
    "name": "srv-ml-stockprice"
  },
  "spec": {
    "replicas": 3,
    "template":{
        "metadata": {           
           "labels": {
               "app": "srv-ml-stockprice",
	       "track": "stable"
            }
         },
        "spec":{
           "containers": [
     	     {
        	"name": "srv-ml-stockprice",
	        "image": "660273858494.dkr.ecr.eu-central-1.amazonaws.com/srv-ml-stockprice:v1",
        	"ports": [
          	    {
            		"containerPort": 80
          	    }
        	]
      	     }
           ]

        }
    }
  }
}
