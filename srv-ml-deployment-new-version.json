{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "namespace":"musicstore",
    "name": "musicstore-website"
  },
  "spec": {
    "replicas": 3,
    "template":{
        "metadata": {           
           "labels": {
               "app": "musicstore-website",
	       "track": "stable"
            }
         },
        "spec":{
           "containers": [
     	     {
        	"name": "frontend",
	        "image": "musicstore:v2",
        	"ports": [
          	    {
            		"containerPort": 80
          	    }
        	]
      	     }
           ]

        }
    }
  }
}
